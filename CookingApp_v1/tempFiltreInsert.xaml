<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CookingApp_v1.tempFiltreInsert">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Inserare Filtre"/>
            <Editor Placeholder="Nume Filtru"
                    Text="{Binding FT_descriere}"
                    HeightRequest="40">
            </Editor>
                <Button Text="Submit" Clicked="OnSubmitButtonClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>