<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CookingApp_v1.RegisterErrorPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Register Error!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Go to Login" Clicked="OnLoginButtonClicked" />
            <Button Text="Go Back" Clicked="OnBackButtonClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>